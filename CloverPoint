	Name = "Clover Point üçÄ",
	Content = "Just a reminder: Clover Point üçÄ is still in Beta so you may expect gliches!",
	Image = "rbxassetid://4483345998",
	Time = 15
})


local credits_Tab = window:MakeTab({
	Name = "Credits üíå",
	PremiumOnly = false
})


local aimbot_Tab = window:MakeTab({
	Name = "Rage ü§¨",
	PremiumOnly = false
})



local player_Tab = window:MakeTab({
	Name = "Player üòé",
	PremiumOnly = false
})


local teleport_Tab = window:MakeTab({
	Name = "Teleport üöÄ",
	PremiumOnly = false
})
credits_Tab :AddParagraph("Credits Alert üíù","Credits to Testoasa#6112 for the script\nCredits to zumo#8222 for the library\nCredits to the Vermilion posts that helped me thru the way\nCredits to NotDyrk for the aimbot!")


teleport_Tab:AddParagraph("Glich Warning ‚ö†","Teleport can sometimes glich and teleport back where your initial position was. This happens due to the anticheat and to prevent that use a slower teleport speed\nIf you are close to where you want to teleport use a high speed but if you are far I recommand a slower one!")
teleport_Tab:AddParagraph("Glich Warning ‚ö†","The speeds I recommand are following: Far(30 - 50), Close(5-20)")



_G.Tp_speed = 0
teleport_Tab:AddSlider({
	Name = "Teleport Speed",
	Min = 1,
	Max = 50,
	Default = 15,
	Color = Color3.fromRGB(74,137,87),
	Increment = 1,
	ValueName = "",
	Callback = function(Value)
		_G.Tp_speed = Value
	end
})


teleport_Tab:AddButton({
	Name = "Sell Ingots",
	Callback = function()   		
local CFrameEnd = CFrame.new(-92.6812668, 4.96235275, 232.330246, 0, 0, 1, 0, 1, -0, -1, 0, 0) -- Place your coords in here
local Time = _G.Tp_speed
local tween =  game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Time), {CFrame = CFrameEnd})
tween:Play()
tween.Completed:Wait()
  	end    
})





















player_Tab:AddParagraph("Glich Warning ‚ö†","The script in still in open Beta so expect gliches!\nFollow the warnings to avoid them happening")
aimbot_Tab:AddParagraph("Glich Warning ‚ö†","The script in still in open Beta so expect gliches!\nFollow the warnings to avoid them happening")






player_Tab:AddLabel("‚ö† Infinite Jump: Mind your jump counter above your stamina!\nOnce it hits 0 you can't jump anymore!")
getgenv().jump = false;
player_Tab:AddToggle({
	Name = "Infinite Jump",
	Default = false,
	Callback = function(bool)
    getgenv().jump = bool
game:GetService("UserInputService").JumpRequest:connect(function()
	if getgenv().jump == true then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
end)
	end    
})




local player = game:GetService("Players").LocalPlayer
getgenv().canGoThru = false;
player_Tab:AddToggle({
	Name = "NoClip",
	Default = false,
	Callback = function(bool)
        getgenv().canGoThru = bool
        if getgenv().canGoThru then
            noclip()
        end
	end    
})
function noclip()
    game:GetService("RunService").Stepped:Connect(function()
        if getgenv().canGoThru == true then
            for i,v in pairs(player.Character:GetDescendants()) do
                if v:IsA("BasePart") and v.CanCollide == true then
                    v.CanCollide = false
                end
            end
        end
    end)
end


player_Tab:AddLabel("‚ö† CFrame Speed: Dont use shift while CFrame speeding!")

Speed = 0.8 
_G.Speed = false;
Player_speed = game.Players.LocalPlayer.Name
UIS = game:GetService("UserInputService")
player_Tab:AddToggle({
	Name = "CFrame Speed",
	Default = false,
	Callback = function(bool)
		_G.Speed = bool
        while _G.Speed == true do
            if UIS:IsKeyDown(Enum.KeyCode.W) then
                game:GetService("Workspace")[Player_speed].HumanoidRootPart.CFrame = game:GetService("Workspace")[Player_speed].HumanoidRootPart.CFrame * CFrame.new(0,0,-Speed)
            end;
            if UIS:IsKeyDown(Enum.KeyCode.A) then
                game:GetService("Workspace")[Player_speed].HumanoidRootPart.CFrame = game:GetService("Workspace")[Player_speed].HumanoidRootPart.CFrame * CFrame.new(-Speed,0,0)
            end;
            if UIS:IsKeyDown(Enum.KeyCode.S) then
                game:GetService("Workspace")[Player_speed].HumanoidRootPart.CFrame = game:GetService("Workspace")[Player_speed].HumanoidRootPart.CFrame * CFrame.new(0,0,Speed)
            end;
            if UIS:IsKeyDown(Enum.KeyCode.D) then
                game:GetService("Workspace")[Player_speed].HumanoidRootPart.CFrame = game:GetService("Workspace")[Player_speed].HumanoidRootPart.CFrame * CFrame.new(Speed,0,0)
            end;
         wait()
     end
end    
})



aimbot_Tab:AddLabel("‚ö† Aimbot: Doesn't work on NPC's parts!")
aimbot_Tab:AddLabel("‚ö† Aimbot: Default bind is E")
aimbot_Tab:AddLabel("‚ö† UNDER WORK ‚ö†")























